{
	"Create Constant": {
	"prefix": "vc",
	"body": [
		"const ${1:Nombre de la Variable} = ${2:Valor de la Variable}",
		"$3"
	],
	"description": "Crea una variable constante"
},

"Arrow Function": {
	"prefix": "flechita",
	"body": [
		"const ${1:Nombre de la funcion} = (${2:Parametros de la funcion}) => $3"
	],
	"description": "Crea una funcion flecha"
},
"Create Boolean": {
	"prefix": "letb",
	"body": [
		"let ${1:variableName} = ${2:true/false}",
		"$3"
	],
	"description": "Crea una variable Booleana"
},
"Create Integer": {
	"prefix": "leti",
	"body": [
		"let ${1:variableName} = ${2:0}",
		"$3"
	],
	"description": "Crea un entero de variable con let"
},
"Create Float": {
	"prefix": "letf",
	"body": [
		"let ${1:variableName} = ${2:0.0}",
		"$3"
	],
	"description": "Crea un decimal de variable con let"
},
"Create Array": {
	"prefix": "letarreglo",
	"body": [
		"let ${1:variableName} = [${2:element1}, ${3:element2}, ${4:element3}]",
		"$5"
	],
	"description": "Crea un arreglo de variables con let"
},
"Create Object": {
	"prefix": "letobjeto",
	"body": [
		"let ${1:variableName} = {",
		"    ${2:key1}: '${3:value1}',",
		"    ${4:key2}: '${5:value2}'",
		"}",
		"$6"
	],
	"description": "Crea un nuevo objeto variable con let"
},
"console.log": {
	"prefix": "log",
	"body": [
		"console.log('$1')"
	],
	"description": "Salida"
},
"Create Object Const": {
	"prefix": "cobj",
	"body": [
		"const ${1:variableName} = {",
		"    ${2:key1}: '${3:value1}',",
		"    ${4:key2}: '${5:value2}'",
		"}",
		"$6"
	],
	"description": "Creates a new object variable"
